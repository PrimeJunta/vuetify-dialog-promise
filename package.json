{
  "name": "vuetify-dialog-promise",
  "version": "2.5.0",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build-bundle": "vue-cli-service build --target lib --name VuetifyDialogPromise ./src/DialogPromise.js",
    "prepare": "npm run build-bundle",
    "test": "vue-cli-service test:unit"
  },
  "main": "./dist/VuetifyDialogPromise.common.js",
  "files": [
    "dist/*",
    "src/*",
    "public/*",
    "*.json",
    "*.js",
    "*.md"
  ],
  "dependencies": {
    "color": "^4.2.3",
    "vue": "^2.6.11",
    "vue-i18n": "^8.24.2",
    "vuetify": "^2.4.0"
  },
  "devDependencies": {
    "@vue/cli-service": "^4.5.0",
    "sass": "^1.32.0",
    "sass-loader": "^10.0.0",
    "vue-cli-plugin-vuetify": "~2.3.1",
    "vue-template-compiler": "^2.6.11",
    "vuetify-loader": "^1.4.3"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "keywords": [
    "vue",
    "vuetify",
    "dialog",
    "promise"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/cloudlace/vuetify-dialog-promise.git"
  }
}
