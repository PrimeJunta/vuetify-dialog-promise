{
  "name": "vuetify-dialog-promise",
  "version": "3.0.2",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build-bundle": "vue-cli-service build --target lib --name VuetifyDialogPromise ./src/DialogPromise.js",
    "test": "vue-cli-service test:unit"
  },
  "main": "./dist/VuetifyDialogPromise.common.js",
  "files": [
    "dist/*",
    "src/*",
    "public/*",
    "*.json",
    "*.js",
    "*.md"
  ],
  "dependencies": {
    "color": "^4.2.3",
    "vue": "^3.1.0",
    "vue-i18n": "^9.2.2",
    "vuetify": "^3.1.4"
  },
  "devDependencies": {
    "@vue/cli-service": "~5.0.8",
    "sass": "^1.32.0",
    "sass-loader": "^10.0.0",
    "vue-cli-plugin-vuetify": "~2.5.8",
    "@vue/compiler-sfc": "^3.1.0",
    "webpack-plugin-vuetify": "^2.0.1"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "keywords": [
    "vue",
    "vuetify",
    "dialog",
    "promise"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/cloudlace/vuetify-dialog-promise.git"
  }
